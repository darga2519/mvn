pipeline {
    agent any

   tools {
        maven 'Maven3.6'
        jdk 'jdk1.8'
    }

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('clean') {
            steps {
	      bat 'mvn clean'
                echo 'Testing..'
            }
        }
        stage('compile') {
            steps {
	      bat 'mvn compile'
                echo 'compiling....'
            }
        }
    }
}
